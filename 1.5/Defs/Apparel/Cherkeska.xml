	<equippedStatOffsets>
		<SocialImpact>0.15</SocialImpact>
		<MentalBreakThreshold>-0.1</MentalBreakThreshold>
	</equippedStatOffsets>

    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
		<li>Neck</li>
		<li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Apparel/Cherkeska/Cherkeska</wornGraphicPath>
      <layers>
		<li>Shell</li>
      </layers>
      <tags>
        <li>MisterM_Cossacks</li>
      </tags>
      <defaultOutfitTags></defaultOutfitTags>
    </apparel>

    <colorGenerator Class="ColorGenerator_StandardApparel" />
